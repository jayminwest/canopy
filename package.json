{
	"name": "@os-eco/canopy-cli",
	"version": "0.1.3",
	"description": "Git-native prompt management for AI agent workflows",
	"type": "module",
	"bin": {
		"cn": "src/index.ts"
	},
	"main": "src/index.ts",
	"files": ["src"],
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"prompt",
		"ai",
		"agent",
		"cli",
		"git",
		"jsonl"
	],
	"author": "Jaymin West",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/jayminwest/canopy.git"
	},
	"engines": {
		"bun": ">=1.0.0"
	},
	"scripts": {
		"test": "bun test",
		"lint": "bunx biome check .",
		"typecheck": "tsc --noEmit",
		"version:bump": "bun run scripts/version-bump.ts"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.4.4",
		"@types/bun": "latest",
		"typescript": "^5.7.3"
	},
	"dependencies": {
		"chalk": "^5.6.2",
		"commander": "^14.0.3"
